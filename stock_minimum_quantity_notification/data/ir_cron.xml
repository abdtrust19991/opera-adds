<odoo noupdate="1">
        <record id="ir_cron_qty_alert_check" model="ir.cron">
            <field name="name">Check Qty Alerts</field>
            <field name="model_id" ref="model_stock_minimum_quantity"/>
            <field name="state">code</field>
            <field name="code">model.check_min_qty()</field>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
</odoo>
